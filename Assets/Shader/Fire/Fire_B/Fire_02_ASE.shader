			half4 frag ( VertexOutput IN  ) : SV_Target
			{
				UNITY_SETUP_INSTANCE_ID( IN );
				UNITY_SETUP_STEREO_EYE_INDEX_POST_VERTEX( IN );

				#if defined(ASE_NEEDS_FRAG_WORLD_POSITION)
				float3 WorldPosition = IN.worldPos;
				#endif
				float4 ShadowCoords = float4( 0, 0, 0, 0 );

				#if defined(ASE_NEEDS_FRAG_SHADOWCOORDS)
					#if defined(REQUIRES_VERTEX_SHADOW_COORD_INTERPOLATOR)
						ShadowCoords = IN.shadowCoord;
					#elif defined(MAIN_LIGHT_CALCULATE_SHADOWS)
						ShadowCoords = TransformWorldToShadowCoord( WorldPosition );
					#endif
				#endif
				float2 texCoord10 = IN.ase_texcoord3.xy * float2( 1,1 ) + float2( 0,0 );
				float2 panner7 = ( 1.0 * _Time.y * _NoiseSpeed + float2( 0,0 ));
				float2 texCoord6 = IN.ase_texcoord3.xy * float2( 1,1 ) + panner7;
				float gradientNoise5 = GradientNoise(texCoord6,_NoiseScale);
				gradientNoise5 = gradientNoise5*0.5 + 0.5;
				float2 temp_cast_0 = (gradientNoise5).xx;
				float2 lerpResult9 = lerp( texCoord10 , temp_cast_0 , _DistortionAmount);
				float4 tex2DNode11 = tex2D( _MainTex, lerpResult9 );
				float time16 = 2.0;
				float2 voronoiSmoothId16 = 0;
				float2 panner14 = ( 1.0 * _Time.y * _VorSpeed + float2( 0,0 ));
				float2 texCoord13 = IN.ase_texcoord3.xy * float2( 1,1 ) + panner14;
				float2 coords16 = texCoord13 * _VorScale;
				float2 id16 = 0;
				float2 uv16 = 0;
				float voroi16 = voronoi16( coords16, time16, id16, uv16, 0, voronoiSmoothId16 );
				
				float3 BakedAlbedo = 0;
				float3 BakedEmission = 0;
				float3 Color = ( ( IN.ase_color * _Color ) * ( tex2DNode11 * ( gradientNoise5 * pow( voroi16 , _Power ) ) ) ).rgb;
				float Alpha = ( tex2DNode11.a * IN.ase_color.a );
				float AlphaClipThreshold = 0.5;
				float AlphaClipThresholdShadow = 0.5;

				#ifdef _ALPHATEST_ON
					clip( Alpha - AlphaClipThreshold );
				#endif

				#ifdef LOD_FADE_CROSSFADE
					LODDitheringTransition( IN.clipPos.xyz, unity_LODFade.x );
				#endif

				#ifdef ASE_FOG
					Color = MixFog( Color, IN.fogFactor );
				#endif

				return half4( Color, Alpha );
			}

			ENDHLSL
		}

		

				float2 voronoiSmoothId36 = 0;
				float2 texCoord44 = IN.ase_texcoord3.xy * _RippleTiling + float2( 0,0 );
				float2 panner43 = ( 1.0 * _Time.y * _RippleSpeed + texCoord44);
				float2 coords36 = panner43 * _RippleScale;
				float2 id36 = 0;
				float2 uv36 = 0;
				float voroi36 = voronoi36( coords36, time36, id36, uv36, 0, voronoiSmoothId36 );
				float2 texCoord54 = IN.ase_texcoord3.xy * float2( 1,1 ) + float2( 0,0 );
				float4 temp_output_52_0 = ( ( _Color * IN.ase_color ) * ( ( pow( voroi36 , _RippleThickness ) * 5.0 ) * pow( ( texCoord54.y - 0.0 ) , _Cutoff ) ) );
				
				float3 BakedAlbedo = 0;
				float3 BakedEmission = 0;
				float3 Color = temp_output_52_0.rgb;
